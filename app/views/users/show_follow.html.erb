<% provide(:title, @title) %>
<section class="stats">
  <%= render 'shared/stats' %>
</section>
<%= will_paginate(@users,
                  renderer: WillPaginate::ActionView::Bootstrap4LinkRenderer) %>
<ul class="list-group list-group-flush">
  <% @users.each do |user| %>
    <li class="list-group-item">
      <%= link_to user.name, user_path(user) %>
    </li>
  <% end %>
</ul>
